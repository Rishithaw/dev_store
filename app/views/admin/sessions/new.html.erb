<h1>Admin Login</h1>

<%= form_with url: admin_login_path, method: :post do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <%= form.submit "Login" %>
<% end %>
