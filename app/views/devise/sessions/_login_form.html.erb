<%= render "devise/sessions/login_errors", resource: resource %>

<%= form_with model: resource,
              as: resource_name,
              url: session_path(resource_name),
              local: true,
              class: "mt-3" do |f| %>

  <%= render "devise/sessions/login_fields", f: f %>

  <div>
    <%= f.submit "Log in", class: "btn btn-primary w-100" %>
  </div>

<% end %>
